<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="com.osdoctor.orm.topicpatient.PatientSection" table="patient_Section">
        <id name="id" type="integer">
            <column name="id" />
            <generator class="native" />
        </id>
      <property name="name" type="string" length="20">
      	<column name="name"/>
      </property>
      <property name="miaoshu" type="string">
      	<column name="miaoshu"></column>
      </property>
      <property name="topicCount" type="int">
      	<column name="topicCount"></column>
      </property>
      <property name="huoyueCount" type="int">
      	<column name="huoyueCount"></column>
      </property>
      <property name="sectionIndex" type="int">
      	<column name="sectionIndex"></column>
      </property>
      <property name="jiansheDate" type="date">
      	<column name="jiansheDate"></column>
      </property>
      <many-to-one name="osControl" class="com.osdoctor.orm.osOperaor.OsControl">
      	<column name="osControlId"></column>
      </many-to-one>
      <set name="topices" order-by="id" lazy="true">
      	<key column="patientSectionId"></key>
      	<one-to-many class="com.osdoctor.orm.topicpatient.PatientTopic"/>
      </set>
    </class>
</hibernate-mapping>
