<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="com.osdoctor.orm.medicineshop.MedicineDayRecommend" table="shop_medicineDayRecommend">
        <id name="id" type="integer">
            <column name="id" />
            <generator class="native" />
        </id>
      <property name="time" type="date">
      	<column name="time"/>
      </property>
      <property name="postion" type="int">
      	<column name="postion"></column>
      </property>
        <many-to-one name="medicineInfo" class="com.osdoctor.orm.medicineshop.MedicineInfo">
        	<column name="medicineInfolId"/>
        </many-to-one>
    </class>
</hibernate-mapping>
