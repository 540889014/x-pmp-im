<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="com.osdoctor.orm.topicdoctormine.DoctorMineTopic" table="topicDoctorMine_doctorMineTopic">
        <id name="id" type="integer">
            <column name="id" />
            <generator class="native" />
        </id>
      <property name="title" type="string">
      	<column name="title"/>
      </property>
      <property name="content" type="string">
      	<column name="content"></column>
      </property>
      <property name="fabiaoTime" type="date">
      	<column name="fabiaoTime"></column>
      </property>
      <property name="patientReplyCount" type="integer">
      	<column name="patientReplyCount"></column>
      </property>
      <property name="views" type="boolean">
      	<column name="views"></column>
      </property>
      <property name="lastUpdateTime" type="date">
      	<column name="lastUpdateTime"></column>
      </property>
      <property name="praiseCount" type="integer">
      	<column name="praiseCount"></column>
      </property>
      <property name="praiseId" type="text">
      	<column name="praiseId"></column>
      </property>
      <many-to-one name="author" class="com.osdoctor.orm.osOperaor.OsDoctor">
      	<column name="osDoctorId"></column>
      </many-to-one>
      <many-to-one name="lastDocReply" class="com.osdoctor.orm.topicdoctormine.PatientMineReply">
      	<column name="lastDocRepalyId"></column>
      </many-to-one>
      <set name="doctorReplies" order-by="id">
      	<key column="patientTopicId"></key>
      	<one-to-many class="com.osdoctor.orm.topicdoctormine.PatientMineReply"/>
      </set>
      <set name="topicLabels" table="topicLabel_DoctorMineTopic">
      	<key column="doctorTopicId"></key>
      	<many-to-many class="com.osdoctor.orm.topicCommon.TopicLabel"  column="topicLabelId"/>
      </set>
    </class>
</hibernate-mapping>
