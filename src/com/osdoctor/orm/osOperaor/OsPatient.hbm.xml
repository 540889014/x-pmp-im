<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="com.osdoctor.orm.osOperaor.OsPatient" table="os_patient">
        <id name="id" type="integer">
            <column name="id" />
            <generator class="native" />
        </id>
      <property name="name" type="string" length="20">
      	<column name="name"/>
      </property>
      <property name="username" type="string" length="20">
      	<column name="username"/>
      </property>
      <property name="password" type="string" length="20">
      	<column name="password"/>
      </property>
      <property name="photoPath" type="string">
      	<column name="photoPath"/>
      </property>
      <property name="personalNote" type="string">
      	<column name="personalNote"></column>
      </property>
      <set name="osDoctors" table="Patient_Doctor">
      	<key column="osPatientId"></key>
      	<many-to-many class="com.osdoctor.orm.osOperaor.OsDoctor"/>
      </set>
      <set name="patientLabels" table="osPatient_TopicLabel">
      	<key column="osPatientId"></key>
      	<many-to-many class="com.osdoctor.orm.topicCommon.TopicLabel" column="topicLabelId"/>
      </set>
    </class>
</hibernate-mapping>
