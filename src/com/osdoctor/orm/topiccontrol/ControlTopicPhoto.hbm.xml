<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="com.osdoctor.orm.topiccontrol.ControlTopicPhoto" table="control_topicphoto">
        <id name="id" type="integer">
            <column name="id" />
            <generator class="native" />
        </id>
 		<property name="photoPath" type="string">
 			<column name="photoPath"></column>
 		</property>
 		<many-to-one name="controlTopic" class="com.osdoctor.orm.topiccontrol.ControlTopic">
 			<column name="controlTopicId"></column>
 		</many-to-one>
    </class>
</hibernate-mapping>
