<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="com.osdoctor.orm.topicCommon.TopicLabel" table="topic_label">
        <id name="id" type="integer">
            <column name="id" />
            <generator class="native" />
        </id>
      <property name="name" type="string" length="20">
      	<column name="name"/>
      </property>
      <property name="CreateTime" type="timestamp">
      	<column name="CreateTime"></column>
      </property>
      <property name="type" type="integer">
      	<column name="type"></column>
      </property>
      <property name="views" type="boolean">
      	<column name="views"></column>
      </property>
      <many-to-one name="osControl" class="com.osdoctor.orm.osOperaor.OsControl">
      	<column name="osControlId"></column>
      </many-to-one>
      <set name="patientTopics" table="topicLabel_PatientTopic">
      	<key column="topicLabelId"></key>
      	<many-to-many class="com.osdoctor.orm.topicpatient.PatientTopic" column="patientTopicId"/>
      </set>
      <set name="doctorMineTopics" table="topicLabel_DoctorMineTopic">
      	<key column="topicLabelId"></key>
      	<many-to-many class="com.osdoctor.orm.topicdoctormine.DoctorMineTopic" column="doctorTopicId"/>
      </set>
      <set name="controlTopics" table="topicLabel_ControlTopic">
      	<key column="topicLabelId"></key>
      	<many-to-many class="com.osdoctor.orm.topiccontrol.ControlTopic" column="controlTopicId"/>
      </set>
    </class>
</hibernate-mapping>
